{
  "hosting": {
    "site": "omnisense",
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }],
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source" : "**",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-store, max-age=0"
      }]
    }]
  }
}